<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
  <security>
    <requestFiltering>
      <requestLimits maxQueryString="32000" maxUrl="32000" />
    </requestFiltering>
  </security>
    <rewrite>
      <rules>
        <rule name="SPA" stopProcessing="true">
          <match url="^(?!.*(.js|.css|.png|.jpg|.ico|.svg)).*$" />
          <conditions logicalGrouping="MatchAll">
          </conditions>
          <action type="Rewrite" url="/"  appendQueryString="true" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>