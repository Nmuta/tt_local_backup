{
  "name": "forza-support-inventory-angular",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "format": "prettier --write .",
    "start": "ng serve",
    "configMoveToBuild": "cp web.config dist/",
    "local:test:chrome": "ng test --karma-config=src/karma.conf.js --source-map=true",
    "local:test:ff": "ng test --karma-config=src/karma-firefox.conf.js --source-map=true",
    "vsts:test": "ng test --code-coverage --karma-config=src/karma-vsts.conf.js",
    "vsts:test:ncc": "ng test --karma-config=src/karma-vsts.conf.js",
    "dev:build": "(ng build --prod --configuration=dev --crossOrigin=anonymous) && npm run configMoveToBuild",
    "dev:zat": "C:\\Windows\\system32\\bash.exe -c \"cd app-local && zat serve\"",
    "dev:start": "ng serve --configuration=dev-api",
    "dev:start:open": "start https://forzasupport1570048282.zendesk.com/agent/apps/scrutineer-inventory-management-angular?zat=true && ng serve --configuration=dev-api",
    "prod:build": "(ng build --prod --configuration=production --crossOrigin=anonymous --subresource-integrity=true) && npm run configMoveToBuild"
  },
