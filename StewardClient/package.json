{
  "name": "turn10-steward-ui",
  "version": "0.0.1",
  "scripts": {
    "refreshVSToken": "vsts-npm-auth -config .npmrc",
    "ng": "ng",
    "fix": "yarn prettier:fix && yarn lint --fix",
    "lint": "ng lint",
    "eslint": "eslint -c ./.eslintrc.js --ext .ts,.html ./**/*.ts",
    "find:missing_no_errors_schema": "grep -rn -C 0 --include \\*.component.spec.ts -L \"\\[NO_ERRORS_SCHEMA\\]\" ./src/*",
    "prettier:fix": "prettier --write .",
    "prettier:check": "prettier --check .",
    "zat": "C:\\Windows\\system32\\bash.exe -c \"cd app-local && zat serve\"",
    "debug-open:ff": "\"C:\\Program Files\\Firefox Developer Edition\\firefox.exe\" -start-debugger-server",
    "start:local-api": "ng serve",
    "start": "ng serve --configuration=dev-api",
    "start:open": "start https://forzasupport1570048282.zendesk.com/agent/apps/scrutineer-inventory-management-angular?zat=true && ng serve --configuration=dev-api",
    "test:cc": "ng test --karma-config=src/karma-coverage.conf.js --source-map=true --code-coverage",
    "test:chrome": "ng test --karma-config=src/karma.conf.js --source-map=true",
    "test:ff": "ng test --karma-config=src/karma-firefox.conf.js --source-map=true",
    "test:ado": "ng test --karma-config=src/karma-ado.conf.js --code-coverage --watch false",
    "test:ado:ncc": "ng test --karma-config=src/karma-ado.conf.js",
    "test:cypress:start": "concurrently \"yarn start\" \"yarn test:cypress\"",
    "test:cypress": "cypress open --config-file cypress/configs/localhost.json",
    "test:cypress:dev": "cypress open --config-file cypress/configs/dev.json",
    "test:cypress:prod": "cypress open --config-file cypress/configs/prod.json",
    "test:cypress:prod-staging": "cypress open --config-file cypress/configs/prod-staging.json",
    "build": "yarn run build:dev",
    "build:dev": "(ng build --prod --configuration=dev --crossOrigin=anonymous) && yarn run build:dev:configMoveToBuild",
    "build:dev:configMoveToBuild": "cp web.config dist/",
    "build:prod": "(ng build --prod --configuration=production --crossOrigin=anonymous --subresource-integrity=true) && yarn run build:dev:configMoveToBuild",
    "open:navbar": "start https://forzasupport1570048282.zendesk.com/agent/apps/scrutineer-inventory-management-angular?zat=true",
    "open:ticket:fm7": "start https://forzasupport1570048282.zendesk.com/agent/tickets/60?zat=true",
    "open:ticket:fh3": "start https://forzasupport1570048282.zendesk.com/agent/tickets/61?zat=true",
    "open:ticket:fh4": "start https://forzasupport1570048282.zendesk.com/agent/tickets/58?zat=true",
    "open:ticket:fh4:bad": "start https://forzasupport1570048282.zendesk.com/agent/tickets/62?zat=true",
    "open:ticket:street": "start https://forzasupport1570048282.zendesk.com/agent/tickets/59?zat=true",
    "open:ticket:street:bad": "start https://forzasupport1570048282.zendesk.com/agent/tickets/57?zat=true"
  },
