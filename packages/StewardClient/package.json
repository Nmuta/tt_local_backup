{
  "name": "turn10-steward-ui",
  "version": "0.0.1",
  "scripts": {
    "auth": "vsts-npm-auth -config .npmrc",
    "auth:force": "vsts-npm-auth -config .npmrc -F",
    "update:browserslists": "npx browserslist --update-db",
    "start": "yarn run start:dev",
    "start:local": "ng serve",
    "start:dev": "ng serve --configuration=dev-api",
    "start:dev:optimized": "ng serve --configuration=dev-api-optimized",
    "start:prod": "ng serve --configuration=prod-api",
    "start:prod:optimized": "ng serve --configuration=prod-api-optimized",
    "ng": "ng",
    "lint": "ng lint",
    "lint:fix": "yarn prettier:fix && yarn lint --fix",
    "prettier:fix": "prettier --write .",
    "build:check:lint": "yarn run ng lint",
    "build:check:prettier": "yarn run prettier --check .",
    "build:check:changelog-is-modified": "C:\\Windows\\system32\\bash.exe ./scripts/changelog-is-modified-or-fail.sh",
    "build:check:ado:changelog-is-modified": "bash ./scripts/changelog-is-modified-or-fail.sh",
    "build": "yarn run build:dev",
    "build:dev": "node --max-old-space-size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=dev --cross-origin=anonymous && yarn run build:configMoveToBuild",
    "build:dev-static": "node --max-old-space-size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=dev-static --cross-origin=anonymous && yarn run build:configMoveToBuild",
    "build:dev-static-pr": "node --max-old-space-size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=dev-static-pr --cross-origin=anonymous && yarn run build:configMoveToBuild",
    "build:dev-api": "node --max-old-space-size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=dev-api --cross-origin=anonymous && yarn run build:configMoveToBuild",
    "build:configMoveToBuild": "cp web.config dist/",
    "build:prod": "node --max-old-space-size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=production --cross-origin=anonymous --subresource-integrity=true && yarn run build:configMoveToBuild",
    "build:prod-api": "node --max-old-space-size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=prod-api --cross-origin=anonymous --subresource-integrity=true && yarn run build:configMoveToBuild",
    "test:prepare": "find src -name '*.scss' -print0 | xargs -0 -I {} bash -c 'cp -f empty.not-scss {}'",
    "test:prepare:add": "git add * && git reset *.scss",
    "test:prepare:reset": "git restore *.scss",
    "test:cc": "ng test --karma-config=src/karma-coverage.conf.js --source-map=true --code-coverage",
    "test:chrome": "ng test --karma-config=src/karma.conf.js --source-map=true",
    "test:ff": "ng test --karma-config=src/karma-firefox.conf.js --source-map=true",
    "test:ado": "ng test --karma-config=src/karma-ado.conf.js --code-coverage --watch false",
    "test:ado:ncc": "ng test --karma-config=src/karma-ado.conf.js",
    "_old-commands-below": "echo Try another command.",
    "__eslint": "eslint -c ./.eslintrc.js --ext .ts,.html ./**/*.ts",
    "find:missing_no_errors_schema": "grep -rn -C 0 --include \\*.component.spec.ts -L \"\\[NO_ERRORS_SCHEMA\\]\" ./src/*",
    "changes-with-main": "git diff-tree --no-commit-id --name-status -r HEAD main",
    "debug-open:ff": "\"C:\\Program Files\\Firefox Developer Edition\\firefox.exe\" -start-debugger-server",
    "_old-aliases-below": "echo Try another command.",
    "refreshVSToken": "yarn run auth:force",
    "fix": "yarn run lint:fix",
    "check:prettier": "yarn run build:check:prettier",
    "check:changelog-is-modified": "yarn run build:check:changelog-is-modified",
    "check:ado:changelog-is-modified": "yarn run build:check:ado:changelog-is-modified"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
