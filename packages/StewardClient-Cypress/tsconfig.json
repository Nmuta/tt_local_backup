{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "@cypress/grep", "node", "cypress-wait-until"],
    "baseUrl": ".",
    "outDir": "./dist/",
    "esModuleInterop": true,
    "paths": {
      "@fixtures/*": ["cypress/fixtures/*"],
      "@plugins/*": ["cypress/plugins/*"],
      "@support/*": ["cypress/support/*"]
    },
  },
  "include": ["cypress/**/*.ts"],
  "exclude": ["node_modules"]
}
