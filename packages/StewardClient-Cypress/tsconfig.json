{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node", "cypress-wait-until"],
    "baseUrl": ".",
    "outDir": "./dist/",
    "paths": {
      "@fixtures/*": ["cypress/fixtures/*"],
      "@plugins/*": ["cypress/plugins/*"],
      "@support/*": ["cypress/support/*"],
      "@cypress/*": ["cypress/*"]
    },
  },
  "include": ["cypress/**/*.ts"]
}
