{
  "name": "turn10-steward-ui-cypress",
  "version": "0.0.1",
  "scripts": {
    "auth": "vsts-npm-auth -config .npmrc",
    "auth:force": "vsts-npm-auth -config .npmrc -F",
    "fix": "yarn prettier:fix && yarn eslint --fix",
    "eslint": "eslint -c ./.eslintrc.js --ext .ts,.html ./**/*.ts",
    "prettier:fix": "prettier --write .",
    "check:prettier": "prettier --check .",
    "check:changelog-is-modified": "C:\\Windows\\system32\\bash.exe ./scripts/changelog-is-modified-or-fail.sh",
    "check:ado:changelog-is-modified": "bash ./scripts/changelog-is-modified-or-fail.sh",
    "open": "cypress open --config-file cypress/configs/localhost.json",
    "open:local": "cypress open --config-file cypress/configs/localhost.json",
    "open:local-dev": "cypress open --config-file cypress/configs/localhost-dev.json",
    "open:dev": "cypress open --config-file cypress/configs/dev.config.ts",
    "open:prod": "cypress open --config-file cypress/configs/prod.config.ts",
    "open:prod-staging": "cypress open --config-file cypress/configs/prod-staging.json",
    "run": "cypress run --config-file cypress/configs/localhost.json",
    "run:local": "cypress run --config-file cypress/configs/localhost.json",
    "run:local-dev": "cypress run --config-file cypress/configs/localhost-dev.json",
    "run:dev": "cypress run --config-file cypress/configs/dev.config.ts",
    "run:prod": "cypress run --config-file cypress/configs/prod.json",
    "run:prod-staging": "cypress run --config-file cypress/configs/prod-staging.json",
    "run:dev:parallel": "cypress-parallel -s run:dev -t 4 -d cypress/e2e"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "cypress": "^12.17.1",
    "cypress-multi-reporters": "^1.6.3",
    "cypress-parallel": "^0.13.0",
    "cypress-real-events": "^1.8.1",
    "cypress-wait-until": "^2.0.0",
    "luxon": "^2.5.2",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 9-11"
  ],
  "resolutions": {
    "jasmine-core": "3.3.0"
  }
}
