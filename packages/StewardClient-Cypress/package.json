{
  "name": "turn10-steward-ui-cypress",
  "version": "0.0.1",
  "scripts": {
    "auth": "vsts-npm-auth -config .npmrc",
    "auth:force": "vsts-npm-auth -config .npmrc -F",
    "lint": "yarn run build:check:eslint && yarn run build:check:prettier",
    "lint:fix": "yarn lint:fix:prettier && yarn lint:fix:eslint",
    "lint:fix:prettier": "prettier --write .",
    "lint:fix:eslint": "yarn eslint --fix",
    "config": "yarn run config:cypress",
    "config:cypress": "bash ./scripts/make-cypress-env.sh",
    "open": "cypress open --config-file cypress/configs/localhost.config.ts",
    "open:local": "cypress open --config-file cypress/configs/localhost.config.ts",
    "open:local-dev": "cypress open --config-file cypress/configs/localhost-dev.config.ts",
    "open:dev": "cypress open --config-file cypress/configs/dev.config.ts",
    "open:prod": "cypress open --config-file cypress/configs/prod.config.ts",
    "open:prod-staging": "cypress open --config-file cypress/configs/prod-staging.config.ts",
    "run": "yarn run:local",
    "run:local": "cypress run --config-file cypress/configs/localhost.config.ts",
    "run:local-dev": "cypress run --config-file cypress/configs/localhost-dev.config.ts",
    "run:dev": "cypress run --config-file cypress/configs/dev.config.ts",
    "run:prod": "cypress run --config-file cypress/configs/prod.config.ts",
    "run:prod-staging": "cypress run --config-file cypress/configs/prod-staging.config.ts",
    "run:dev:parallel": "cypress-parallel -s run:dev -t 4 -d cypress/e2e",
    "run:changes-only": "bash ./scripts/execute-changed-specs.sh",
    "build": "yarn tsc",
    "build:check:eslint": "yarn run eslint",
    "build:check:prettier": "prettier --check .",
    "report:delete": "rm -r reports/ || true",
    "report:combine:dev": "jrm reports/mocha-junit/dev.xml \"reports/mocha-junit/dev/junit-*.xml\"",
    "report:combine:prod": "jrm reports/mocha-junit/prod.xml \"reports/mocha-junit/prod/junit-*.xml\"",
    "_old-commands-below": "echo Try another command.",
    "eslint": "eslint -c ./.eslintrc.js --ext .ts,.html ./**/*.ts",
    "prettier:fix": "yarn run lint:fix:prettier"
  },
  "private": true,
  "dependencies": {
    "@types/node": "^20.4.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/eslint-plugin-tslint": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "cypress": "^12.17.1",
    "cypress-multi-reporters": "^1.6.3",
    "cypress-parallel": "^0.13.0",
    "cypress-real-events": "^1.8.1",
    "cypress-wait-until": "^2.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^46.4.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "junit-report-merger": "^6.0.2",
    "luxon": "^2.5.2",
    "mocha-junit-reporter": "^2.2.1",
    "prettier": "^3.0.0",
    "typescript": "^4.6.2"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 9-11"
  ],
  "resolutions": {
    "jasmine-core": "3.3.0"
  }
}
