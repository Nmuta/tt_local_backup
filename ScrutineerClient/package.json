{
  "name": "forza-support-inventory-angular",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "format": "prettier --write .",
    "start": "ng serve",
    "configMoveToBuild": "cp web.config dist/",
    "local:test:chrome": "ng test --karma-config=src/karma.conf.js --source-map=true",
    "local:test:ff": "ng test --karma-config=src/karma-firefox.conf.js --source-map=true",
    "vsts:test": "ng test --code-coverage --karma-config=src/karma-vsts.conf.js",
    "vsts:test:ncc": "ng test --karma-config=src/karma-vsts.conf.js",
    "dev:build": "(ng build --prod --configuration=dev --crossOrigin=anonymous) & npm run configMoveToBuild",
    "prod:build": "(ng build --prod --configuration=production --crossOrigin=anonymous --subresource-integrity=true) & npm run configMoveToBuild"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.2",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.2",
    "@angular/compiler": "~8.2.2",
    "@angular/core": "~8.2.2",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.2.2",
    "@angular/material": "^8.1.3",
    "@angular/platform-browser": "~8.2.2",
    "@angular/platform-browser-dynamic": "~8.2.2",
    "@angular/pwa": "~0.803.12",
    "@angular/router": "~8.2.2",
    "@angular/service-worker": "~8.2.2",
    "@azure/msal-angular": "^1.0.0",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@nguniversal/express-engine": "~8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "~8.1.1",
    "@ngxs/router-plugin": "^3.4.3",
    "@ngxs/store": "^3.4.3",
    "core-js": "^3.2.1",
    "hammerjs": "^2.0.8",
    "msal": "^1.3.3",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zendesk_app_framework_sdk": "^2.0.11",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.26",
    "@angular/cli": "~8.2.2",
    "@angular/compiler-cli": "~8.2.2",
    "@angular/platform-server": "8.2.2",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@ngxs/devtools-plugin": "^3.4.3",
    "@types/applicationinsights-js": "~1.0.9",
    "@types/gapi.youtube": "~3.0.34",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "12.7.2",
    "@types/youtube": "~0.0.35",
    "codelyzer": "^5.0.1",
    "copyfiles": "~2.1.0",
    "express-logging": "~1.1.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "~1.2.0",
    "karma-nightmare": "~0.4.15",
