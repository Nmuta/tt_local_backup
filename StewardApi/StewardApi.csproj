    <PackageReference Include="Turn10.Data.Common" Version="2.1.0" />
    <PackageReference Include="Turn10.Data.KustoLogger" Version="4.0.0" />
    <PackageReference Include="Turn10.Services.ForzaClient" Version="1.4.0" />
    <PackageReference Include="Turn10.Services.ForzaClient.FH3" Version="1.1.0" />
